{
  "name": "@readme/data-urls",
  "version": "3.0.1",
  "description": "A utility for parsing and validating data URLs.",
  "license": "ISC",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/data-urls.git"
  },
  "bugs": {
    "url": "https://github.com/readmeio/data-urls/issues"
  },
  "homepage": "https://github.com/readmeio/data-urls#readme",
  "scripts": {
    "build": "tsup",
    "format": "npm run prettier && npx biome check --write",
    "lint": "biome check",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@readme/standards": "^2.1.1",
    "@types/node": "^24.0.8",
    "@vitest/coverage-v8": "^4.0.2",
    "prettier": "^3.0.3",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6",
    "vitest": "^4.0.2"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": "@readme/standards/prettier"
}
