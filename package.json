{
  "name": "@readme/data-urls",
  "version": "3.0.0",
  "description": "A utility for parsing and validating data URLs.",
  "license": "ISC",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/data-urls.git"
  },
  "bugs": {
    "url": "https://github.com/readmeio/data-urls/issues"
  },
  "homepage": "https://github.com/readmeio/data-urls#readme",
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .js,.ts",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "vitest --coverage",
    "test:browser": "vitest --browser.name=chrome --browser.headless"
  },
  "devDependencies": {
    "@readme/eslint-config": "^14.0.0",
    "@types/node": "^22.0.2",
    "@vitest/browser": "^2.0.5",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint": "^8.57.0",
    "prettier": "^3.0.3",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6",
    "vitest": "^2.0.5"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": "@readme/eslint-config/prettier"
}
